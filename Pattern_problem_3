import java.util.Scanner;

/*Character Pattern
A A A A
B B B B
C C C C
D D D D
* */
public class Pattern_problem_2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
            char ch = (char)('A'+ i-1);
            for(int j=1;j<=n;j++){
                System.out.print(ch);
            }
            System.out.println();
        }

    }
}
